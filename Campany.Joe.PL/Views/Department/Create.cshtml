@model CreateDepartmentDtos

<h1>Create Department</h1>

<br />
<br />


<form asp-action="Create">
    <div class="my-3">
        <label asp-for="Code" class="col-form-label"></label>
        <input asp-for="Code"  class="form-control"/>
        <span asp-validation-for="Code"></span> @*Return Inside Of ErrorMessage*@
    </div>
    <div class="my-3">
        <label asp-for="Name" class="col-form-label"></label>
        <input asp-for="Name" class="form-control" />
        <span asp-validation-for="Name"></span>  @*Return Inside Of ErrorMessage*@
    </div>
    <div class="my-3">
        <label asp-for="CreateAt" class="col-form-label"></label>
        <input asp-for="CreateAt" class="form-control" />
        <span asp-validation-for="CreateAt"></span> @*Return Inside Of ErrorMessage*@
    </div>
    <div>
        <input type="submit" value="Create" class="btn btn-primary" />
        <a asp-action="Index" class="btn btn-light">Back To List</a>

    </div>

</form>