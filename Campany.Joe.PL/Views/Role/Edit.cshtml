@model RoleToReturnDto
@{
    ViewData["Title"] = "Edit";
}

<h1>Update Page</h1>

<br />
<br />


<form asp-action="Edit" enctype="multipart/form-data">   
    <partial name="EmployeePartialViews/_CreateEditPartialView" />

   
    <div>
      
        <input type="submit" value="Save" class="btn btn-warning" />
        <a asp-action="Index" class="btn btn-light">Back To List</a>

    </div>
    <br />
    <div>
        <a asp-action="AddOrRemoveUsers" asp-route-roleId="@Model.Id" class="btn btn-success">Add Or Remove User</a>
    </div>

</form>
@section ValidationSection {
    <script src="~/lib/jquery-validation/dist/jquery.validate.min.js"></script>
    <script src="~/lib/jquery-validation-unobtrusive/jquery.validate.unobtrusive.min.js"></script>
}